            android:name=".UI.Dashboard.ViewBuyerReport.ViewReportByDateActivity"
            android:parentActivityName=".UI.Dashboard.ViewBuyerReport.ViewBuyerReportActivity"
            android:windowSoftInputMode="adjustNothing" />
